<div class="block mx-auto my-12 p-8 bg-white w-1/3 border border-gray-200 rounded-lg shadow-lg"/>
    <h1 class="text-center text-3xl font-bold">Editar Usuario</h1>
    {{#each usuarios}}
    <form action="" method="POST" class="mt-4">
        <input type="number" class="border border-gray-200 rounded-md bg-gray-200 w-full text-lg placeholder-gray-500 p-2 my-2 focus:bg-white" placeholder="Carnet de identidad" name="ciUsuario" value="{{ciUsuario}}">
        <input type="text" class="border border-gray-200 rounded-md bg-gray-200 w-full text-lg placeholder-gray-500 p-2 my-2 focus:bg-white" placeholder="Nombres" name="nombre" value="{{nombre}}">    
        <input type="text" class="border border-gray-200 rounded-md bg-gray-200 w-full text-lg placeholder-gray-500 p-2 my-2 focus:bg-white" placeholder="Apellido principal" name="apellidoPrincipal" value="{{apellidoPrincipal}}"> 
        <input type="text" class="border border-gray-200 rounded-md bg-gray-200 w-full text-lg placeholder-gray-500 p-2 my-2 focus:bg-white" placeholder="Apellido secundario" name="apellidoSecundario" value="{{apellidoSecundario}}">
        <input type="number" class="border border-gray-200 rounded-md bg-gray-200 w-full text-lg placeholder-gray-500 p-2 my-2 focus:bg-white" placeholder="Telefono" name="telefono" value="{{telefono}}">
        <input type="text" class="border border-gray-200 rounded-md bg-gray-200 w-full text-lg placeholder-gray-500 p-2 my-2 focus:bg-white" placeholder="Fecha" name="nacimiento" id="nacimiento" value="{{nacimiento}}">  
        <input type="password" class="border border-gray-200 rounded-md bg-gray-200 w-full text-lg placeholder-gray-500 p-2 my-2 focus:bg-white" placeholder="ContraseÃ±a" name="password">  
        <button type="submit" class="rounded-md bg-green-500 w-full text-lg text-white font-semibold p-2 my-3 hover:bg-green-600 focus:bg-green-700">Registrar</button>
    </form>
    {{/each}}
</div>